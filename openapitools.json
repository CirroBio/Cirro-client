{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.2.0",
    "generators": {
      "cirro": {
        "generatorName": "python",
        "inputSpec": "https://api.cirro.bio/openapi/cirro-data-latest.yml",
        "typeMappings": {
          "date": "string"
        },
        "additionalProperties": {
          "packageName": "cirro_api_client"
        }
      }
    }
  }
}
